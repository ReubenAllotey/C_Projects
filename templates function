#include <iostream>
#include <string>

using namespace std;

template<typename t>
void print_arr(t* arr, int len){
    for ( int i=0;i<len; ++i)
        cout<< *(arr+i)<<" "<<endl;

}




int main (){

int arr1[]={5 ,6,8,9,2,1};
print_arr<int>(arr1,6);

float arr2[]={2.45f,8.6f,2.9f};
print_arr<float>(arr2,3);


    
return 0;
}
